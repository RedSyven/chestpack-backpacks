{
    "model": {
        "type": "minecraft:select",
        "property": "minecraft:display_context",
        "cases": [
            {
                "when": "gui",
                "model": {
                    "type": "model",
                    "model": "rbp:item/backpack_chest"
                }
            },
            {
                "when": "fixed",
                "model": {
                    "type": "model",
                    "model": "rbp:item/backpack_chest"
                }
            }
        ],
        "fallback": {
            "type": "minecraft:model",
            "model": "rbp:item/3d_chest"
        }
    }
}